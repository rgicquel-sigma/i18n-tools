{
    "compileOnSave": false,
    "compilerOptions": {
        "baseUrl": "./src",
        "downlevelIteration": true,
        "module": "ESNext",
        "outDir": "./dist",
        "sourceMap": false,
        "declaration": false,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "target": "ES2018",
        "typeRoots": [
            "node_modules/@types"
        ],
        "lib": [
            "ES2018"
        ]
    }
}
